{
    "name": "leroy-merlin-br/multitude",
    "description": "A Leroy Merlin's project",
    "keywords": ["framework", "laravel", "lumen"],
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "predis/predis": "~1.0",
        "illuminate/redis": "5.2.*",
        "vluzrmos/tinker": "^1.1",
        "leroy-merlin-br/mongolid": "2.0.x-dev",
        "cviebrock/laravel-elasticsearch": "^1.2",
        "phpfluent/elastic-query-builder": "^1.0",
        "zircote/swagger-php": "^2.0",
        "mockery/mockery": "^0.9.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Leadgen\\": "Leadgen/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
